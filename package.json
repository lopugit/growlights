{
  "name": "growlights",
  "version": "0.0.1",
  "description": "Any and All Grow Products in Australia",
  "productName": "Grow Lights Australia",
  "cordovaId": "org.cordova.quasar.app",
  "author": "lopugit <ireply@myleisure.com.au>",
  "private": true,
  "scripts": {
    "test": "echo \"No test specified\" && exit 0",
    "dev": "npm run start",
    "start": "cross-env level=dev apiTLD=src domain=growlights pm2 start ecosystem.config.js ; pm2 logs growlights",
    "prod": "cross-env level=prod apiSubdomain='beta.api' quasar build -m pwa",
    "start-papi": "cross-env level=prod quasar start",
    "start-bapi": "cross-env level=build quasar start",
    "start-dapi": "cross-env level=dev apiTLD=src domain=growlights quasar start",
    "build": "cross-env level=prod quasar build",
    "build-papi": "cross-env level=prod quasar build",
    "build-bapi": "cross-env level=build quasar build",
    "build-dapi": "cross-env level=dev apiTLD=src domain=growlights quasar build",
    "commit": "git add . ; git commit -m 'ready'",
    "push": "npm run commit ; git push origin HEAD",
    "pull": "git pull origin HEAD"
  },
  "dependencies": {
    "@quasar/extras": "1.0.0",
    "axios": "0.21.1",
    "deepmerge": "3.2.0",
    "vue-i18n": "7.3.3"
  },
  "devDependencies": {
    "@firebase/firestore": "2.3.8",
    "@quasar/app": "1.0.0-beta.19",
    "alopu-socket.io-parser": "3.1.2",
    "circular-json": "0.5.1",
    "cross-env": "5.2.0",
    "firebase": "8.7.1",
    "firebaseui": "4.8.1",
    "flatted": "2.0.0",
    "fs": "0.0.1-security",
    "grow.ai": "0.0.11",
    "has-binary2": "1.0.3",
    "lodash": "4.17.11",
    "lopu3": "0.0.13",
    "node-sass": "4.14.1",
    "pug": "2.0.3",
    "pug-loader": "2.3.0",
    "pug-plain-loader": "1.0.0",
    "sass-loader": "6.0.7",
    "smarts": "1.0.156",
    "socket.io-client": "2.0.4",
    "strip-ansi": "3.0.1",
    "ua-parser-js": "0.7.17",
    "uuid": "3.3.2",
